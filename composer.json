{
    "name": "popphp/pop-auth",
    "description": "Pop Auth Component for Pop PHP Framework",
    "keywords": [
        "authentication",
        "authorization",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/pop-auth",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "popphp/pop-db": "^6.5.11",
        "popphp/pop-http": "^5.2.11"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Auth\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Auth\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
