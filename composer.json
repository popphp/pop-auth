{
    "name": "popphp/pop-auth",
    "description": "Pop Auth Component for Pop PHP Framework",
    "keywords": [
        "authentication",
        "authorization",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "New BSD",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "popphp/pop-crypt": "2.*@dev",
        "popphp/pop-db": "2.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Auth\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Auth\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
